# Custom interface file generated by Cobalt

auto lo
iface lo inet loopback

# We don't want eth0 as auto here. We use ifplugd which to handle eth0
# link up and down events.
auto eth0
iface eth0 inet dhcp
	address 192.168.1.85
	netmask 255.255.255.0
	gateway 192.168.1.85
	dns-nameservers 192.168.1.1

